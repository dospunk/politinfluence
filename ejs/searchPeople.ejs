<!DOCTYPE html>
<head>
	<title>Politinfluence | Search People</title>
</head>
<body>
	<%if(list.length > 0){ %>
		<ul>
			<% list.forEach(function(doc){%>
				<li> <a href="/person?id=<%= doc._id %>&pageNum=1&mode=votes"><%= doc.name %></a>
					<ul>
						<li> <%= doc.state %>, district <%= doc.district %> </li>
					</ul>
				</li>
			<% }); %>
		</ul>
	<% } if(list.length === 0) { %>
		<h3>No Results Found.</h3>
	<% } %>
</body>